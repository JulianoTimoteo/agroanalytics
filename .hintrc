{
  "extends": [
    "development"
  ],
  "hints": {
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "meta[name=theme-color]"
        ]
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-webkit-backdrop-filter"
        ]
      }
    ],
    "no-inline-styles": "off",
    "axe/keyboard": [
      "default",
      {
        "nested-interactive": "off"
      }
    ]
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not chrome <= 112",
    "not ios_saf <= 26",
    "not safari <= 26.1",
    "not samsung <= 29",
    "not and_chr <= 142",
    "not and_ff <= 144",
    "not chrome <= 142",
    "not edge <= 142",
    "not firefox <= 144",
    "not opera <= 122"
  ]
}